<script type="text/javascript" src="{{ editor.libUri }}"></script>
<script type="text/javascript">
    var wysiwyg = new ihqs.wysiwyg(
        "{{ selector }}",
        {
            library: "{{ editor.name }}",
            set:     "{{ editor.set }}",
            theme:   "{{ editor.theme }}"
        }
    );
    wysiwyg.initHandler();
</script>