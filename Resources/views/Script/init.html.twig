<script type="text/javascript" src="{{ editor.libUri }}"></script>
<script type="text/javascript">
    var wysiwygLoad = function()
    {
        var wysiwyg = new ihqs.wysiwyg(
            "{{ selector }}",
            {
                library: "{{ editor.name }}",
                set:     "{{ editor.settings }}",
                theme:   "{{ editor.theme }}"
            }
        );
        wysiwyg.initHandler(); 
    }

    $(document).ready(wysiwygLoad);
</script>